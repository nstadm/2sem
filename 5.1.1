#include <iostream>
#include <string>
using namespace std;

class Person {
public:
    string name;
    int age;
    int IP;
};

void print(Person* person) {
    cout << person->name << " is " << person->age << " years old, number IP " << person->IP << endl;
}

int main() {
    Person person;
    person.name = "Harry";
    person.age = 23;
    person.IP = 9889;

    cout << "Meet " << person.name << endl;
    print(&person);

    return 0;
}
